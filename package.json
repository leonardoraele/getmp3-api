{
  "name": "getmp3-api",
  "version": "0.1.0",
  "description": "Download YouTube videos' audio as mp3 files.",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "per-env",
    "serve:production": "node . | pino-pretty -c -t",
    "serve:development": "node-dev --inspect=${NODE_INSPECT_HOST:-127.0.0.1} . | pino-pretty -c -t",
    "test": "echo \"tests not implemented yet\" ; exit 1",
    "postinstall": "link-module-alias"
  },
  "keywords": [
    "mp3",
    "youtube",
    "converter",
    "downloader"
  ],
  "author": "Leonardo Raele <leonardoraele@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "commander": "^8.1.0",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "fastify": "^3.20.2",
    "ffmpeg-static": "^4.4.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.21",
    "per-env": "^1.0.2",
    "pino-pretty": "^5.1.3",
    "prism-media": "^1.3.2",
    "require-all": "^3.0.0",
    "streaming-cache": "^0.5.5",
    "throw2": "^1.0.1",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "node-dev": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:leonardoraele/mp3youtube.git"
  },
  "_moduleAliases": {
    "lib": "lib",
    "config": ".config.js"
  }
}
